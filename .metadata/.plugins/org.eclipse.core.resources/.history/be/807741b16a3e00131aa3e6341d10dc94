package ast.bool;

import ast.arith.ArithExpr;
import ast.dynamicAnalysis.Environment;
import ast.dynamicAnalysis.VariableNotDefinedException;

public class NotEqualsExpr extends BoolExpr {

	private ArithExpr expression1;
	private ArithExpr expression2;
	
	public NotEqualsExpr(ArithExpr expression1, ArithExpr expression2) {
		this.expression1 = expression1;
		this.expression2 = expression2;
	}
	
	@Override
	public boolean evaluate(Environment env) throws VariableNotDefinedException {
		return expression1.evaluate(env) != expression2.evaluate(env);
	}

}
